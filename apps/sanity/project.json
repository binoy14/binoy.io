{
  "name": "sanity",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/sanity/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm sanity build dist/apps/sanity -y"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm sanity dev"
      }
    },
    "deploy-graphql": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm sanity graphql deploy"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm tsc -p apps/home/tsconfig.json --noEmit"
      }
    }
  },
  "tags": []
}
